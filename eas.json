{
  "cli": {
    "version": ">= 15.0.6",
    "appVersionSource": "remote",
    "requireCommit": "true"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal"
    },
    "preview": {
      "distribution": "internal"
    },
    "apk": {
      "android": {
        "buildType": "apk"
      }
    },
    "testflight": {
      "ios": {
        
      }
    },
    "production": {
      "ios": {
        
      },
      "android": {
        "buildType": "app-bundle"
      }
    }
  },
  "submit": {
    "testflight": {
      "ios": {
        "appleId": "YOUR_APPLE_ID@example.com",
        "ascAppId": "YOUR_APP_STORE_CONNECT_APP_ID",
        "appleTeamId": "YOUR_APPLE_TEAM_ID"
      }
    },
    "production": {
      "ios": {
        "appleId": "YOUR_APPLE_ID@example.com",
        "ascAppId": "YOUR_APP_STORE_CONNECT_APP_ID",
        "appleTeamId": "YOUR_APPLE_TEAM_ID"
      },
      "android": {
        "track": "production"
      }
    }
  }
} 